{
  "name": "fastify-aws-jwt-verify",
  "version": "0.1.0",
  "description": "Fastify plugin providing AWS Cognito token verification for authentication/authorization",
  "author": "GrayBrace",
  "homepage": "https://github.com/graybrace/fastify-aws-jwt-verify",
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "keywords": [
    "fastify",
    "fastify-plugin",
    "aws cognito"
  ],
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graybrace/fastify-aws-jwt-verify.git"
  },
  "bugs": {
    "url": "https://github.com/graybrace/fastify-aws-jwt-verify/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc -p tsconfig.prod.json",
    "check:knip": "knip",
    "check:lint": "eslint .",
    "checks": "run-s check:*",
    "pretest": "rimraf ./coverage && rimraf ./reports",
    "test": "jest --config jest.config.cjs --no-cache"
  },
  "dependencies": {
    "aws-jwt-verify": "^4.0.0",
    "fastify-plugin": "^4.5.1",
    "http-errors": "^2.0.0"
  },
  "peerDependencies": {
    "fastify": "^4.0.0"
  },
  "devDependencies": {
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "fastify": "^4.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "knip": "^3.8.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
